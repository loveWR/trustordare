<view class="page">
    <view class="page__bd">
        <view class="weui-grids">
            <block wx:for="{{grids}}" wx:key="*this">
                <view url="" class="weui-grid" hover-class="weui-grid_active">
                    <image class="weui-grid__icon" src="{{item.qrcode}}"  bindtap='to' data-id="{{item.appid}}"/>
                    <view class="weui-grid__label">{{item.title}}</view>
                </view>
            </block>
        </view>
    </view>
</view>

 <ad class='ad' unit-id="{{ad_id}}"></ad>